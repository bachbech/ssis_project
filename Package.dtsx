ions du Masque'&#xA;    WHEN publisher = '?' AND title LIKE '%Anita Blake%' THEN 'Milady'&#xA;    WHEN publisher = '?' AND title LIKE '%Anouna%' THEN 'Éditions du Seuil'&#xA;    WHEN publisher = '?' AND title LIKE '%Assassin Royal%' THEN 'Pocket Jeunesse'&#xA;    WHEN publisher = '?' AND title LIKE '%Autre-Monde%' THEN 'Pocket Jeunesse'&#xA;    -- Add more WHEN clauses for other publishers&#xA;    ELSE publisher -- Leave other publisher values unchanged&#xA;  END;&#xA;&#xA;&#xA;UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET publisher = &#xA;  CASE &#xA;    WHEN publisher = '?' AND title LIKE '%Amos Daragon%' THEN &#xA;      CASE &#xA;        WHEN title LIKE '%Les Intouchables%' THEN 'Les Intouchables (édition originale)'&#xA;        WHEN title LIKE '%Éditions AdA%' THEN 'Éditions AdA (nouvelle édition)'&#xA;      END&#xA;    WHEN publisher = '?' AND title LIKE '%Andy Brazil%' THEN 'Éditions du Masque'&#xA;    WHEN publisher = '?' AND title LIKE '%ANGE%' THEN 'Éditions du Masque'&#xA;    WHEN publisher = '?' AND title LIKE '%Anita Blake%' THEN 'Milady'&#xA;    WHEN publisher = '?' AND title LIKE '%Anouna%' THEN 'Éditions du Seuil'&#xA;    WHEN publisher = '?' AND title LIKE '%Assassin Royal%' THEN 'Pocket Jeunesse'&#xA;    WHEN publisher = '?' AND title LIKE '%Autre-Monde%' THEN 'Pocket Jeunesse'&#xA;    WHEN publisher = '?' AND title LIKE '%Axis-03% L''homme étoile' THEN 'Éditions du Masque'&#xA;    WHEN publisher = '?' AND title LIKE '%Axis-1% Tranchant d''Acier' THEN 'Éditions du Masque'&#xA;    WHEN publisher = '?' AND title LIKE '%Base Vénus-5% La Lune de Diamant' THEN 'Fleuve Noir'&#xA;    WHEN publisher = '?' AND title LIKE '%Bernhard Gunther-03% Un requiem allemand' THEN 'Fleuve Noir'&#xA;    WHEN publisher = '?' AND title LIKE '%Bernhard Gunther-04% La mort, entre autres' THEN 'Fleuve Noir'&#xA;    WHEN publisher = '?' AND title LIKE '%Bernhard Gunther-1% L''été de cristal' THEN 'Fleuve Noir'&#xA;   &#xA;    ELSE publisher &#xA;  END;&#xA;&#xA;UPDATE 