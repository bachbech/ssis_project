          SQLTask:SqlStatementSource="select distinct &quot;authors&quot; from [Fantastic].[dbo].[OLE DB Destination]&#xA;group by authors&#xA;&#xA;&#xA; UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET authors = REPLACE(LTRIM(RTRIM(authors)), '  ', ' ')&#xA;&#xA;UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET authors = REPLACE(REPLACE(authors, 'Ã©', 'e'), 'Ã¨', 'e');&#xA;&#xA;UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET authors = REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(&#xA;    authors,&#xA;    'à‰', 'É'),&#xA;    'à§', 'ç'),&#xA;    'â€™', '’'),&#xA;    'à®', 'î'),&#xA;    'à ', 'à'),&#xA;    'Æ', 'œ'),&#xA;    'Ë', 'è'),&#xA;    'Ê', 'ê'),&#xA;    'TÊ¿', 'Tʼ');&#xA;&#xA;UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET authors = REPLACE(REPLACE(authors, 'Ã©', 'e'), 'Ã¨', 'e');&#xA;&#xA;UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET [authors] = REPLACE([authors], 'Ã¤', 'a')&#xA;WHERE [authors] LIKE '%Ã¤%';&#xA;&#xA;SELECT authors&#xA;FROM [Fantastic].[dbo].[OLE DB Destination]&#xA;WHERE authors LIKE '%[^a-zA-Z0-9 ]%';&#xA;&#xA;UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET [authors] = REPLACE(REPLACE([authors], 'Ã©', 'e'), 'Ã¨', 'e')&#xA;WHERE [authors] LIKE '%Ã%' OR [authors] LIKE '%e%'; -- Adjust as needed&#xA;&#xA;&#xA;&#xA;UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET AuthorFirst = NULL, AuthorMiddle = NULL, AuthorLast = NULL;&#xA;&#xA;UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET AuthorFirst = PARSENAME(REPLACE(authors, ';', '.'), 3),&#xA;    AuthorMiddle = PARSENAME(REPLACE(authors, ';', '.'), 2),&#xA;    AuthorLast = PARSENAME(REPLACE(authors, ';', '.'), 1);&#xA;&#xA;&#x9;UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET&#xA;  AuthorFirst = CASE WHEN CHARINDEX(',', authors) &gt; 0 THEN SUBSTRING(authors, 1, CHARINDEX(',', authors) - 1) ELSE authors END,&#xA;  AuthorMiddle = CASE&#xA;                   WHEN CHARINDEX(',', authors) &gt; 0 AND CHARINDEX('&amp;', authors) &gt; CHARINDEX(',', authors) THEN&#xA;                     SUBSTRING(authors, CHARINDEX(',', authors) + 1, CHARINDEX('&amp;', authors) - CHARINDEX(',', authors) - 1)&#xA;                   ELSE&#xA;                     NULL  -- or any default value you want when the pattern is not found&#xA;                 END,&#xA;  AuthorLast = CASE WHEN CHARINDEX('&amp;', authors) &gt; 0 THEN SUBSTRING(authors, CHARINDEX('&amp;', authors) + 1, LEN(authors) - CHARINDEX('&amp;', authors)) ELSE NULL END&#xA;WHERE CHARINDEX(',', authors) &gt; 0 OR CHARINDEX('&amp;', authors) &gt; 0;&#xA;&#xA;UPDATE [Fantastic].[dbo].[OLE DB Destination]&#xA;SET&#xA;  AuthorMiddle = CASE&#xA;                   WHEN CHARINDEX(',', authors) &gt; 0 THEN SUBSTRING(authors, CHARINDEX(',', authors) + 1, LEN(authors) - CHARINDEX(',', authors))&#xA;                   ELSE NULL&#xA;                 END&#xA;WHERE CHARINDEX(',', authors) &gt; 0;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
